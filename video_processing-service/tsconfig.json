{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "esModuleInterop": true,
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      //"outFile": "../../built/local/tsc.js",
      "sourceMap": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["**/*.spec.ts","node_modules"]
  }